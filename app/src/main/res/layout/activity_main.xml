<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativelayout"
    android:background="#A6A6A6"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/et1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp" />

    <Button
        android:id="@+id/btnclear"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:layout_below="@+id/et1"
        android:layout_marginTop="160dp"
        android:onClick="btnclear"
        android:text="c"
        android:textSize="24sp"
        app:backgroundTint="#F4511E"
        tools:ignore="UsingOnClickInXml" />

    <Button
        android:id="@+id/button1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="5dp"
        android:onClick="btndot"
        android:text="."
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:onClick="btn0"
        android:text="0"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:onClick="btneql"
        android:text="="
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#F4511E" />

    <Button
        android:id="@+id/button4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:onClick="btndiv"
        android:text="/"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#8C8C8C" />

    <Button
        android:id="@+id/button5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button1"
        android:layout_marginBottom="5dp"
        android:onClick="btn7"
        android:text="7"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button2"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:onClick="btn8"
        android:text="8"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button3"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:onClick="btn9"
        android:text="9"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button4"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:onClick="btnmulti"
        android:text="*"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#8C8C8C" />

    <Button
        android:id="@+id/button9"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button5"
        android:layout_marginBottom="5dp"
        android:onClick="btn4"
        android:text="4"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button10"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button6"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:onClick="btn5"
        android:text="5"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button11"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button7"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:onClick="btn6"
        android:text="6"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button12"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button8"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:onClick="btnsub"
        android:text="-"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#8C8C8C" />

    <Button
        android:id="@+id/button13"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button9"
        android:layout_marginBottom="5dp"
        android:onClick="btn1"
        android:text="1"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF"
        android:textColor="#000000" />

    <Button
        android:id="@+id/button14"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button10"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:onClick="btn2"
        android:text="2"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button15"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button11"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:onClick="btn3"
        android:text="3"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#FFFFFF" />

    <Button
        android:id="@+id/button16"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/button12"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:onClick="btnadd"
        android:text="+"
        android:textColor="#000000"
        android:textSize="24sp"
        app:backgroundTint="#8C8C8C" />



</RelativeLayout>